    "gentypes": "graphql-codegen --config codegen.yml",
    "watch": "tsc -w",
    "lint": "eslint --ext .ts,.tsx,.js src/",
    "format": "yarn lint --fix",
    "deploy": "git push heroku master",
    "postinstall": "node ./configs/install/install.js",
    "test": "echo 'test'"
